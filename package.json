{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://benthorn.github.io/portfolio-site",
  "dependencies": {
    "concurrently": "^4.0.1",
    "fs": "0.0.1-security",
    "jquery": "^3.3.1",
    "jquery-address": "^1.6.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1",
    "sass": "^1.14.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass": "sass --watch src/sass/styles.scss src/App.css",
    "dev": "concurrently --kill-others-on-fail \"npm run sass\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
}
